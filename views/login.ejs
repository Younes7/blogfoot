<%include ./partials/header%>
<div class="row">
  <div class="col-md-8 center">
    <form class="col-4 center" action="/login" method="post">
      <div class="form-group">
        <label for="name">Nom</label>
        <input type="name" class="form-control" id="name" name="name" aria-describedby="emailHelp" placeholder="Votre Nom">
      </div>
      <div class="form-group">
        <label for="password">Mot de passe</label>
        <input type="password" class="form-control" name="password" id="password" placeholder="Votre mot de passe">
      </div>
      <button type="submit" name="submit" id="submit" class="btn btn-info">Se connecter</button>
    </form>
  </div>
</div>
<%include ./partials/footer%>
